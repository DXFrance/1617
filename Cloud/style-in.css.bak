	*
{
	margin:0;
	padding:0
}
.circle:hover {
    background: #3498db;
    -webkit-animation: pulse 0.7s;
    animation: pulse 0.7s;
}
.crossSell {
	text-align: left;
	display: inline-block;
}
div.sticky-wrapper nav .container .image a {
	display: block;
	width: 100%;
	height: 100%;
} 
.circle {
    width: 40px;
    height: 40px;
    background: #fff;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    display: inline-block;
    text-align: center;
    margin: auto 5px;
position: relative;
}
.circle i {
    font-size: 25px;
    position: relative;
    top: 50%;
    transform: translateY(-50%);
}


.badge-link .tooltiptext {
    visibility: hidden;
    width: 120px;
    background-color: black;
    color: #fff;
    text-align: center;
    border-radius: 6px;
    padding: 5px 0;
    position: absolute;
    z-index: 1;
    bottom: 150%;
    left: 50%;
    margin-left: -60px;
}

.ui-tooltip {
	z-index: 5182749837214097219 !important;
}

.badge-link .tooltiptext::after {
    content: "";
    position: absolute;
    top: 100%;
    left: 50%;
    margin-left: -5px;
    border-width: 5px;
    border-style: solid;
    border-color: black transparent transparent transparent;
}

.badge-link:hover .tooltiptext {
    visibility: visible;
}
	body,html
{
	margin:0;
	padding:0;
	height:100%;
	font-size:1em;
	width:auto;
	color:#3f3f3f;
	font-family:"Segoe UI",sans-serif;
	background-color:#fff
}
.logo
{
	background-color:transparent;
	background-image:url("../../images/general/logo.png");
    width: 92px;
    height: 25px;
	background-position:center center;
	background-repeat:no-repeat
}
.btn
{
	background-color:#89c402;
	    border-radius:0px;
	color:#fff;
	margin-right:1em;
	text-transform: uppercase;
	font-weight:bold;
	transition: .2s ease-out;
	box-shadow: 0 2px 5px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12);
    cursor: pointer;
	letter-spacing: .5px;
	font-size:1.1em
}
.btn:hover
{
	color:#fff;
	background-color:#89c402;
	box-shadow: 0 5px 11px 0 rgba(0,0,0,0.18),0 4px 15px 0 rgba(0,0,0,0.15);
	-webkit-animation:pulse 0.7s;
	animation:pulse 0.7s
}
nav .content a {
    transition: 0.2s linear all;
    position: relative;
    bottom: -5px;
		   font-weight: bold;
		color: #FFF;
		padding: 5px 11px;
	/*	border-radius: 4px; */
}

nav .content a.active {
	background: white;
    color: #3F3F3F;
    font-weight: bold;
}
	nav
{
	background-color:#1a1a1a;
}
	div.sticky-wrapper nav .container .content
{
	display:inline-block;
	margin:0 auto;
    float:right;
	padding:0px 13px 15px
}
	div.sticky-wrapper.is-sticky nav
{
	display:block;
	height:50px;
	position: fixed;
  top: 0px;
	display:none;
}

.badge-home {
    display: inline-block;
    width: 50px;
    height: 50px;
    position: fixed;
    bottom: 20px;
    left: 20px;
    background: white;
    z-index: 123;
    color: #1da1f2;
    text-align: center;
    font-size: 25px;
    line-height: 50px;
    border-radius: 100%;
    cursor: pointer;
    -moz-box-shadow: 0px 0px 5px 0px #656565;
    -webkit-box-shadow: 0px 0px 5px 0px #656565;
    -o-box-shadow: 0px 0px 5px 0px #656565;
    box-shadow: 0px 0px 5px 0px #656565;
    filter:progid:DXImageTransform.Microsoft.Shadow(color=#656565, Direction=NaN, Strength=5);
		transition: linear all 0.2s;
}

.badge-home:hover {
	bottom: 25px;
}

.badge-home a {
	display: block;
	width: 100%;
	height: 100%;
}

.badge-home i {
	color: #1da1f2;
}

